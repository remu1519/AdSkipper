{
    "name": "AdSkipper",
    "version": "1.0.0",
	"author": "Megumi",
    "manifest_version": 2,
    "description": "Ad Skip for YouTube.",
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["adskipper.js"]
    }],
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png",
        "512": "icon_512.png"
    },
    "permissions": ["activeTab"]
  }
